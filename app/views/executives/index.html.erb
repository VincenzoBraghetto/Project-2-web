<div style= "margin-left: 30px;">
<p style="color: green"><%= notice %></p>

<h1>Executives</h1>

<div id="executives">

  <% @executives.each do |executive| %>
    <ul>
      <li>
        <h5><%= link_to executive.name+" "+executive.last_name, executive, style: "color: inherit; text-decoration: none;" %></h5>
      </li>
    </ul>
  <% end %>
</div>

<%= link_to "New executive", new_executive_path %>
</div>
